<%- include ("./partials/header"); -%>
<h1>Compose</h1>
<form action="/compose" method="POST">
    <!-- form-group, form-control, btn btn-btn bootstrap classes -->
    <div class="form-group">
        <label>Title</label>
        <input name="postTitle" type="text" class="form-control">
        <label>Post</label>
        <textarea name="postBody" cols="40" rows="5" class="form-control"></textarea>
    </div>
    <button type="submit" class="btn btn-btn">Publish</button>
</form>
<%- include ("./partials/footer"); -%>